{
  "name": "@brightspace-ui/visual-diff",
  "version": "1.2.1",
  "description": "Visual difference utility using Mocha, Chai, Puppeteer, and PixelMatch",
  "repository": "https://github.com/BrightspaceUI/visual-diff.git",
  "publishConfig": {
    "access": "public"
  },
  "main": "visual-diff.js",
  "scripts": {
    "lint": "eslint . --ext .js,.html",
    "test": "npm run lint",
    "test:diff": "mocha './test/**/*.visual-diff.js' -t 10000",
    "test:diff:golden": "mocha './test/**/*.visual-diff.js' -t 10000 --golden",
    "test:diff:golden:commit": "node ./bin/commit-goldens.js"
  },
  "bin": {
    "commit-goldens": "bin/commit-goldens.js"
  },
  "files": [
    "file-helper.js",
    "s3-helper.js",
    "visual-diff.js"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-eslint": "^10",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.9",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "mocha": "^7",
    "puppeteer": "^2",
    "simple-git": "^1.126.0"
  },
  "peerDependencies": {
    "mocha": "^7",
    "puppeteer": "^2"
  },
  "dependencies": {
    "aws-sdk": "^2",
    "chai": "^4",
    "chalk": "^4",
    "es-dev-server": "^1",
    "pixelmatch": "^5"
  }
}
